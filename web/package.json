{
  "name": "website",
  "version": "1.16.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "start": "[ -d \"./dist\" ] && servor dist index.html 3000 || echo \"build has not been run\"",
    "dev": "webpack-dev-server",
    "build": "NODE_ENV=production webpack && echo 'User-agent: *\nDisallow: /' > dist/robots.txt",
    "build:test": "NODE_ENV=test webpack",
    "prebuild": "rimraf dist",
    "prettier": "prettier --write 'src/{**/*,*}.{ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "test": "echo \"No tests specified\"",
    "e2e": "cypress open",
    "parseProps": "node -r esbuild-register scripts/parse-props/index.ts",
    "new": "node scripts/cli",
    "release": "node -r esbuild-register scripts/release.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "arrowParens": "always"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
